<% cache "entries/js/tagged/#{@tag_slug}/page/#{@page}/count/#{@count}/#{@photoblog.id}/#{@photoblog.updated_at.to_i}" do %>
  <%= render partial: 'entries/entry_list_item',
      collection: @entries,
      as: :entry,
      locals: {
        page_url: @page == 1 ? tag_url(tag: @tags.first.slug, page: nil) : tag_url(@tags.first.slug, @page),
        lazy_load: true,
        square: false,
        observe: true
      } %>
<% end %>
