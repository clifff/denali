<% cache "entries/js/page/#{@page}/count/#{@count}/#{@photoblog.id}/#{@photoblog.updated_at.to_i}" do %>
  <%= render partial: 'entries/entry_list_item',
      collection: @entries,
      as: :entry,
      locals: {
        page_url: @page == 1 ? entries_url(page: nil) : entries_url(page: @page),
        lazy_load: true,
        square: false,
        observe: true
      } %>
<% end %>
