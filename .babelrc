{
  "presets": [
    ["env", {
      "modules": false,
      "targets": {
        "browsers": ["last 1 version", "last 3 safari versions", "last 3 ios versions"],
        "uglify": true
      },
      "useBuiltIns": true
    }]
  ],

  "plugins": [
    "syntax-dynamic-import",
    "transform-object-rest-spread",
    ["transform-class-properties", { "spec": true }]
  ]
}
